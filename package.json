{
  "name": "cbrwizard.com",
  "devDependencies": {
    "browserify": "^8.1.3",
    "browserify-incremental": "^1.4.0",
    "es6ify": "1.6.0",
    "watchify": "^2.3.0"
  },
  "license": "MIT",
  "engines": {
    "node": ">= 0.10"
  },
  "scripts": {
    "bundle": "browserify app/assets/javascripts/index.js -o app/assets/javascripts/bundle.js -t es6ify --extension=\".js.es6\"",
    "watch-js": "watchify app/assets/javascripts/index.js -o app/assets/javascripts/bundle.js",
    "start": "npm run watch-js & rails s"
  },
  "browserify": {
    "transforms": [
      [
        "es6ify"
      ]
    ]
  },
  "dependencies": {
    "es6ify": "1.6.0"
  }
}
